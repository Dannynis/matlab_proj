
clear all; close all;
load('LPF.mat');

figure
suptitle('Absolute Frequency Response')


h2_l = length(h2)

w = linspace(-pi,pi,h2_l)

fft_h2 =  abs(fftshift(fft(h2)))

plot(w,fft_h2)
title('H2');
xlabel('[f]')
ylabel('Amplitude')



